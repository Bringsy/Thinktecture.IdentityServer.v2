@using Thinktecture.IdentityServer.Web.Utility
@model bool
@{
    ViewBag.Title = "Cancel";
}

@if (Model)
{
    <h2>Registration Cancelled</h2>
    <p>
        Your account has been closed.
        If you would like to re-register click <a href="@Url.Action("index", "signup")@HttpContext.Current.Request.Url.Query">here</a>.
    </p>
    <p>Thanks!</p>
}
else
{
    <h2>There was an error canceling your account.</h2>
}